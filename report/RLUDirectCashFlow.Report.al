    local procedure get101week1(): Decimal
    var
        BankAccountLedgerEntry: Record "Bank Account Ledger Entry";
    begin
        if getstartweek2() <> 0D then begin
            BankAccountLedgerEntry.SetRange("Bank Account No.", "Bank Account Ledger Entry"."Bank Account No.");
            BankAccountLedgerEntry.SetRange("CF Code", '101');
            BankAccountLedgerEntry.SetFilter("Posting Date", '%1..%2', getstartweek1(), CalcDate('<-1D>', getstartweek2()));
            if BankAccountLedgerEntry.FindFirst() then begin
                BankAccountLedgerEntry.CalcSums(Amount);
                exit(BankAccountLedgerEntry.Amount);
