    procedure texttodate(tanggalregis: Text): Date
    var
        yourdate: date;
        yourtextdate: Text;
        tanggal: Text;
        bulan: Text;
        tahun: Text;

    begin
        // yourtextdate := '01/04/2020';

        // Evaluate(calon.TanggalRegistrasi, yourtextdate);
        Clear(yourtextdate);
        tahun := tanggalregis.Substring(1, 4);
        bulan := tanggalregis.Substring(5, 2);
        tanggal := tanggalregis.Substring(7, 2);

        yourtextdate := tahun + '-' + bulan + '-' + tanggal;
        Evaluate(yourdate, yourtextdate);
        exit(yourdate)

    end;

    local procedure getstartweek1(): Date
    var
        daterec1: Record date;
    begin
        daterec1.SetRange("Period Type", daterec1."Period Type"::Date);
        daterec1.SetRange("Period Name", 'Monday');
        daterec1.SetFilter("Period Start", '%1..%2', varstartdate, CalcDate('<+7D>', varstartdate));
        if daterec1.FindFirst() then
            exit(daterec1."Period Start");
    end;

